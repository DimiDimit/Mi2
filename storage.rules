rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /artists/{uid}/{path=**} {
      allow read;
      allow write: if request.auth.uid == uid;
    }
    match /profiles/{uid} {
    	match /avatar {
      	allow read;
        allow write: if request.auth.uid == uid;
      }
    	match /{path=**} {
      	allow read, write: if request.auth.uid == uid;
      }
    }
  }
}
